	"config": {
		"commitizen": {
			"path": "git-cz"
		}
	},
	"devDependencies": {
		"@popperjs/core": "^2.11.6",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@types/svelte-leafletjs": "^0.8.2",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.13",
		"classnames": "^2.3.2",
		"commitizen": "^4.2.5",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"flowbite": "^1.5.4",
		"flowbite-svelte": "^0.28.4",
		"git-cz": "^4.9.0",
		"leaflet": "^1.9.3",
		"postcss": "^8.4.19",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"svelte": "^3.53.1",
		"svelte-check": "^2.9.2",
		"svelte-leafletjs": "^0.10.0",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.2.4",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^3.2.4"
	},
	"type": "module"
}
